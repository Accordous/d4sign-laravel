{
    "name": "accordous/d4sign-laravel",
    "description": "A D4Sign é uma plataforma de assinatura eletrônica de documentos que atende os requisitos da Medida Provisória 2.200-2/01, ou seja, autenticidade, integridade e não repúdio. Trazendo, assim, validade jurídica para todas as assinaturas realizadas através da D4Sign.",
    "license": "MIT",
    "authors": [
        {
            "name": "edmarr2",
            "email": "edmar_espinola@hotmail.com"
        },
        {
            "name": "Lúdio",
            "email": "ludio@accordous.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Accordous\\D4sign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accordous\\D4sign\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Accordous\\D4sign\\Providers\\D4SignServiceProvider"
            ]
        }
    }
}
